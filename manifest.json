{
   "background": {
      "page":"public/option.html"
   },
   "browser_action": {
      "defaul_icon": "img/128x128.png",
      "default_popup": "public/option.html"
   },
   "content_scripts": [ {
      "js": [ "injeccion/content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Modulo de ajedrez para aprender",
   "icons": {
      "128": "img/128x128.png",
      "16": "img/16x16.png",
      "64": "img/64x64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Module chess",
   "permissions": [ "storage","notifications","tabs","http://*/*","https://*/*","cookies"],
   "short_name": "Chess module",
   "version": "6.2.1"
}
